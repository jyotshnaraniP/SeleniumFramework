<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Automation Test Suite">
<listeners>
<listener class-name="MyAcademy.listeners"></listener>
<listener class-name="MyAcademy.ExtentReportsListeners"></listener>
</listeners>
  
  <test name="Validating User Login">
    <classes>
      <class name="MyAcademy.HomePage"/>
      </classes>
  </test>
  
  <test name="App Testing">
  	  <classes>
      <class name="MyAcademy.AppTest"/>
      </classes>
  </test>
       
  <test name="Validating page Title">
      <classes>
      <class name="MyAcademy.ValidateTitle"/>
      </classes>
  </test>
  
  <test name="Validating Navigation Bar">
      <classes>
      <class name="MyAcademy.ValidateNavigationBar"/>
    </classes>
  </test>
</suite>
